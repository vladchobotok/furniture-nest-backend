    "fastify-multipart": "^5.4.0",
    "ioredis": "^5.3.2",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.3.1",
    "pg": "^8.10.0",
    "pg-hstore": "^2.3.4",
    "redis": "^4.6.7",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
