    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "^9.0.0",
    "@nestjs/jwt": "^10.0.3",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/platform-fastify": "^9.4.1",
    "@nestjs/sequelize": "^9.0.2",
    "@nestjs/serve-static": "^3.0.1",
    "@nestjs/swagger": "^6.3.0",
    "@types/multer": "^1.4.7",
    "bcrypt": "^5.1.0",
    "bcryptjs": "^2.4.3",
